#: src/main/help.c:21
#, c-format
msgid "Usage: %s [OPTION] [FILE]..."
msgstr "Utilisation : %s [OPTIONS] [FICHIER]..."

#: src/main/help.c:23
msgid ""
"Concatenate FILE(s), or standard input, to standard output,\n"
"with monitoring."
msgstr ""
"Concatène FICHIER(s), ou l'entrée standard, sur la sortie standard\n"
"avec monitorage."

#: src/main/help.c:27
msgid "-p, --progress        show progress bar"
msgstr "-p, --progress        affiche la barre de progression"

#: src/main/help.c:28
msgid "-t, --timer           show elapsed time"
msgstr "-t, --timer           affiche le temps écoulé"

#: src/main/help.c:29
msgid "-e, --eta             show estimated time of arrival (completion)"
msgstr ""
"-e, --eta             affiche l'heure approximative de l'achèvement de la\n"
"                        tâche"

#: src/main/help.c:30
msgid "-r, --rate            show data transfer rate counter"
msgstr "-r, --rate            affiche le taux de tranfert des données"

#: src/main/help.c:31
msgid "-b, --bytes           show number of bytes transferred"
msgstr "-b, --bytes           affiche le nombre d'octets transférés"

#: src/main/help.c:32
msgid "-f, --force           output even if standard error is not a terminal"
msgstr ""
"-f, --force           imprime vers la sortie d'erreur même si ce n'est pas\n"
"                        un terminal"

#: src/main/help.c:33
msgid "-n, --numeric         output percentages, not visual information"
msgstr ""
"-n, --numeric         imprime en pourcentage, pas les informations visuelles"

#: src/main/help.c:34
msgid "-q, --quiet           do not output any transfer information at all"
msgstr "-q, --quiet           n'afficher aucune information de transfert"

#: src/main/help.c:35
msgid "-c, --cursor          use cursor positioning escape sequences"
msgstr ""
"-c, --cursor          utiliser les séquences d'échappements de\n"
"                        positionnement de curseur"

#: src/main/help.c:36
msgid "-L, --rate-limit RATE limit transfer to RATE bytes per second"
msgstr ""
"-L, --rate-limit TAUX limite le taux de transfer à TAUX octets par seconde"

#: src/main/help.c:37
msgid "-W, --wait            display nothing until first byte transferred"
msgstr ""
"-W, --wait            ne rien afficher avant qu'au moins un octet soit\n"
"                        tranféré"

#: src/main/help.c:38
msgid "-s, --size SIZE       set estimated data size to SIZE bytes"
msgstr ""
"-s, --size TAILLE     ajuste la taille estimée des données à TAILLE octets"

#: src/main/help.c:39
msgid "-i, --interval SEC    update every SEC seconds"
msgstr "-i, --interval SEC    mise-à-jour toute les SEC secondes"

#: src/main/help.c:40
msgid "-w, --width WIDTH     assume terminal is WIDTH characters wide"
msgstr ""
"-w, --width LARGEUR   présumer la largeur du terminal à LARGEUR caractères"

#: src/main/help.c:41
msgid "-N, --name NAME       prefix visual information with NAME"
msgstr "-N, --name NOM        préfixer les informations visuelles avec NOM"

#: src/main/help.c:43
msgid "-h, --help            show this help and exit"
msgstr "-h, --help            afficher cette aide puis quitter"

#: src/main/help.c:44
msgid ""
"-l, --license         show the license this program is distributed under"
msgstr ""
"-l, --license         affiche la license sous laquelle ce programme est\n"
"                        distribué"

#: src/main/help.c:45
msgid "-V, --version         show version information and exit"
msgstr "-V, --version         afficher la version puis quitter"

#: src/main/help.c:46
#, c-format
msgid "Please report any bugs to %s."
msgstr "SVP rapporter touts bogues à %s."

